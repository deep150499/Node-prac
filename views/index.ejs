<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs')  %>
<body>

    

     <%- include('./partials/nav.ejs') %>

    <div>
        <h3>All Blogs</h3>
   

    <% if(blogs.length > 0) { %>
        <% blogs.forEach(blog => { %>
            <h3><%= blog.title  %></h3>
            <p><%= blog.snippet %></p>
        <% }); %>
    <% } else { %>
        <p>No blogs to display</p>
    <% } %>
    </div>
    <%- include('./partials/footer.ejs')  %>
</body>
</html>